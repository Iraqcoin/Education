      <script type="text/javascript" src="{{static_domain}}/js/angular.min.js"></script>
      <script type="text/javascript" src="{{static_domain}}/js/angular-route.min.js"></script>
    
      <script type="text/javascript">
      var app = angular.module('eduCore', []);

       app.config(function($interpolateProvider, $httpProvider) {
            $interpolateProvider.startSymbol('[[').endSymbol(']]');
            $httpProvider.defaults.headers.post['Content-Type'] = 'application/x-www-form-urlencoded;charset=utf-8';
            $httpProvider.defaults.transformRequest = function(obj) {
                var str = [];
                for (var p in obj) {
                    str.push(encodeURIComponent(p) + "=" + encodeURIComponent(obj[p]));
                }
                return str.join("&");
            };
        });

      app.controller('courseCtr', ['$scope' , '$rootScope','$http', function($scope,$rootScope,$http){
         $scope.id = "{{id}}";
         $scope.listCourse = [];
         $scope.temp = [];
         console.log($scope.id);

         $scope.getDataList = function(){
               $scope.showLoading = true;
               $scope.getJSON("{{domain}}/api?action=get-course-detail&page=1&maxRow=100&parentID=" + $scope.id , function(data){
                    if(data.data){
                        $scope.temp = data.data;
                        for(var x in $scope.temp){
                           var id = $scope.temp[x].id;
                           $scope.getDetails($scope.temp[x],id);
                        }
                    }
                    $scope.showLoading = false;
                }, function(error){
                    console.log(error);
                });
         }

         $scope.getDetails = function(parent,id){
               if($scope.id != 0 && id != 0){
                  console.log("get list detail");
                  $scope.getJSON("{{domain}}/api?action=get-detail&page=1&maxRow=100&parentID=" + id , function(data){
                     if(data.data){
                           parent.details = data.data;
                           $scope.listCourse.push(parent);
                     }
                  }, function(error){
                          console.log(error);
                  });
               }
         }

         

         $scope.cacheAjax = {};

         $scope.getJSON = function (url, success , error) {
                //Get from cache
                if (typeof $scope.cacheAjax[url] != "undefined") {
                    //console.log("getfromCache");
                    success($scope.cacheAjax[url]);
                    return;
                }
                $http.get(url)
                    .success(function (data) {
                        if (typeof data !== "undefined") {
                            var tmp;
                            tmp = angular.copy(data, tmp);
                            $scope.cacheAjax[url] = tmp;
                            try {
                                success(data);
                            } catch (e) {
                                console.log("error");
                            }
                        }
                    }).error(function(){
                        console.log("Get JSON Ajax Fail: " + url);
                        error("Get JSON Ajax Fail: " + url);
                    });
            }//end GetJson
            $scope.getDataList();

      }])
      </script>
